@page "/adminmanageusers"
@rendermode InteractiveServer
@inject NavigationManager Nav
<PageTitle>AdminManageUsers</PageTitle>

<h3>Admin Manage Users</h3>

<p>This is a page dedicated to allowing the admins to add and remove users/monitor user activity</p>

<EditForm Model="@myModel">
    <InputText @bind-Value="myModel.MyTextProperty" placeholder="Enter text here" />
</EditForm>


<button @onclick="HandleButtonClick">Click Me!</button>
<p>@myModel.MyTextProperty</p>



@code {
    private ManageUsers manageUsers; 
    private MyDataModel myModel = new MyDataModel();

    public class MyDataModel
    {
        public string MyTextProperty { get; set; }
    }
    
    private string message = "Button not clicked yet.";

    protected override void OnInitialized()
    {
        // Pass the injected NavigationManager into your class
        manageUsers = new ManageUsers(Nav);
    }

    public void HandleButtonClick()
    {
        manageUsers.AddUser(myModel.MyTextProperty);
    }

    
    
}
